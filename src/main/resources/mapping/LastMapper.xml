<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hx.dao.LastMapper" >
  <resultMap id="BaseResultMap" type="com.hx.model.Last" >
  </resultMap>

  <insert id="insertLoginLog" parameterType="com.hx.model.Last">
    insert  into
    last(id,userid,username,territory_ip,landing_time)
    values (#{id},#{userid},#{username},#{territory_ip},#{landing_time})
  </insert>

</mapper>